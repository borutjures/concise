    "compile:runWatch": "yarn compile:run -- --watch"
  },
  "devDependencies": {
    "babel-runtime": "^6.23.0",
    "concise-types": "*"
  },
  "dependencies": {
    "graphql": "^0.9.3",
    "graphql-tools": "^0.11.0",
    "sequelize": "^3.30.4",
    "sqlite3": "^3.1.8",
    "concise-yaml": "*",
    "concise": "*",
    "concise-json": "*",
    "concise-pg": "*",
    "concise-flow": "*",
    "concise-graphql": "*",
    "concise-diagram": "*",
    "concise-sequelize": "*"
  }
}
