{
  "name": "concise-examples",
  "private": true,
  "version": "0.0.0",
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "scripts": {
    "compile": "yarn compile:clean && yarn compile:run",
    "compileWatch": "yarn compile:clean && yarn compile:runWatch",
    "compile:clean": "rm -rf ./lib",
    "compile:run": "../../node_modules/.bin/babel src -d lib --ignore \"**/__mocks__/**\",\"**/__tests__/**\"",
    "compile:runWatch": "yarn compile:run -- --watch"
  },
  "devDependencies": {
    "babel-runtime": "^6.23.0",
    "concise-types": "^0.0.0"
  },
  "dependencies": {
    "concise-yaml": "^0.0.0",
    "concise": "^0.0.0",
    "concise-json": "^0.0.0",
    "concise-pg": "^0.0.0",
    "concise-flow": "^0.0.0",
    "concise-graphql": "^0.0.0",
    "concise-svg": "^0.0.0"
  }
}
